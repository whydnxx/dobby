{
  "name": "@whydnxx/dobby",
  "version": "1.0.0",
  "description": "whydn Eslint configuration",
  "public": true,
  "author": "whydnxx",
  "license": "MIT",
  "keywords": [
    "whydnxx",
    "eslint",
    "stylelint",
    "commitlint",
    "javascript"
  ],
  "main": "index.js",
  "scripts": {
    "deduplicate": "npx yarn-deduplicate && yarn install",
    "up-latest": "yarn upgrade-interactive --latest",
    "lint": "eslint --fix \"*.js\"",
    "lint:fix": "yarn lint --fix && yarn format",
    "prepare": "husky install $PWD/.husky",
    "prepublishOnly": "yarn lint",
    "release": "np",
    "ship": "np",
    "test": "yarn lint",
    "validate": "yarn lint"
  },
  "homepage": "https://github.com/whydnxx/dobby#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whydnxx/dobby.git"
  },
  "bugs": {
    "url": "https://github.com/whydnxx/dobby.git/issues"
  },
  "dependencies": {
    "eslint-config-kentcdodds": "^19",
    "eslint-config-prettier": "^8",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-prettier": "^4",
    "eslint-plugin-react": "^7",
    "eslint-plugin-simple-import-sort": "^7",
    "prettier": "^2",
    "typescript": "^4"
  },
  "devDependencies": {
    "eslint": "^8.5.0",
    "husky": "^7",
    "lint-staged": "^12",
    "np": "^7"
  },
  "peerDependencies": {
    "eslint": "^7"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "./index.js"
    ],
    "ignorePatterns": [
      "examples"
    ],
    "root": true
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "np": {
    "branch": "main"
  },
  "prettier": {
    "endOfLine": "auto",
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "*.js"
  ],
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ]
}
